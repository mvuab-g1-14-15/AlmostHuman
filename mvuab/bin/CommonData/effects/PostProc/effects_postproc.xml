<effects>	
	<technique name="VignettingTechnique" vertex_type="1104">
		<effect name="VignettingEffect" file="../CommonData/effects/PostProc/Vignetting.fx"/>
		<handles/>
	</technique>
	
	<technique name="BlurTechnique" vertex_type="1104">
		<effect name="BlurEffect" file="../CommonData/effects/PostProc/Blur.fx"/>
		<handles/>
	</technique>
	
	<technique name="SobelTechnique" vertex_type="1104">
		<effect name="SobelEffect" file="../CommonData/effects/PostProc/Sobel.fx"/>
		<handles/>
	</technique>
    
    <technique name="PerlinNoiseTechnique" vertex_type="1104">
		<effect name="PerlinNoiseEffect" file="../CommonData/effects/PostProc/PerlinNoise.fx"/>
		<handles use_time="true" use_inverse_projection_matrix="true" use_inverse_view_matrix="true"/>
	</technique>
    
    <technique name="ScatteredLightTechnique" vertex_type="1104">
		<effect name="ScatteredLightEffect" file="../CommonData/effects/PostProc/ScatteredLight.fx"/>
		<handles use_world_view_projection_matrix="true" use_lights="true" num_of_lights="1"/>
	</technique>

	<technique name="FogTechnique" vertex_type="1104">
		<effect name="FogEffect" file="../CommonData/effects/PostProc/Fog.fx"/>
		<handles use_inverse_projection_matrix="true" use_inverse_view_matrix="true" use_camera_position="true" fog_start="50.0" fog_end="150.0" fog_exp = "0.02" fog_fun="1"/>
	</technique>
	
	<technique name="SSAOTechnique" vertex_type="1104">
		<effect name="SSAOEffect" file="../CommonData/effects/PostProc/SSAO.fx"/>
		<handles use_inverse_projection_matrix="true" use_inverse_view_matrix="true"/>
	</technique>
	
	<technique name="LensFlareTechnique" vertex_type="1104">
		<effect name="LensFlareEffect" file="../CommonData/effects/PostProc/LensFlare.fx"/>
		<handles/>
	</technique>
	
	<technique name="BloomFilter" vertex_type="1104">
		<effect name="BloomEffectFilter" file="../CommonData/effects/PostProc/BloomGaussianCombine.fx"/>
		<handles/>
	</technique>
	
	<technique name="CombineBloomTechnique" vertex_type="1104">
		<effect name="BloomEffectGaussianCombine" file="../CommonData/effects/PostProc/BloomGaussianCombine.fx"/>
		<handles/>
	</technique>
	
	<technique name="GaussianBlur" vertex_type="1104">
		<effect name="GaussianBlurEffect" file="../CommonData/effects/PostProc/GaussianBlur.fx"/>
		<handles/>
	</technique>
		
</effects>