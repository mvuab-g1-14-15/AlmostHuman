<emitter active="true" name="spiral4" loop="false" time_to_live="5.0">
	<particles max_alive_particles="300" time_to_live="3 3">
		<technique name="ParticleTechniqueV2" render_group="alpha_blend"/>
		<texture name="../CommonData/sparkle.png" flip_uv_horizontal="true" flip_uv_vertical="true"/>
		<interpolators>
			<interpolator alpha="0.0" color="0.3 0.3 0.3" size="0.05"/>
			<interpolator alpha="0.2" color="0.5 0.5 0.5" size="0.05"/>
			<interpolator alpha="0.0" color="0.6 0.6 0.6" size="0.05"/>
		</interpolators>
		<motion linear_speed="0.4 0.6" initial_dir_min="0 0.1 0" initial_dir_max="0 0.1 0" radial_speed="20 40" gravity="-1"/>
	</particles>
	<emissions random_initial_angle="true" emission_time="0.1" particles_per_emission="1 3" cubic_size="0.1 0.0 0.1"/>
</emitter>