<?xml version="1.0" encoding="iso-8859-1"?>
<Actions>
	<action name="ShowHideLogRender">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_F1"/>
    </action>
	
	<action name="PgUpLogRender">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_PGUP"/>
    </action>
	
	<action name="PgDownLogRender">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_PGDOWN"/>
    </action>
	
    <action name="CommutationCamera">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_G"/>
	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN_UP"  Code="KEY_H"/>
    </action>
	
	<action name="ReloadActionToInput">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN_UP"  Code="KEY_R"/>
    </action>
	
	<action name="ReloadStaticMesh">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN_UP"  Code="KEY_F9"/>
    </action>
	
	<action name="ReloadLUA">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN_UP"  Code="KEY_F10"/>
    </action>
	
	<action name="ReloadShaders">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN_UP"  Code="KEY_F11"/>
    </action>
	
	<action name="SaveActionToInput">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN_UP"  Code="KEY_S"/>
	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_LCTRL"/>
    </action>
	
	<action name="SetActionToInput">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN_UP"  Code="KEY_F"/>
	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_LCTRL"/>
    </action>
	
	<action name="MoveForward">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_W"/>
    </action>
	
	<action name="MoveBackward">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_S"/>
    </action>
	
	<action name="MoveLeft">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_A"/>
    </action>
	
	<action name="MoveRight">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_D"/>
    </action>
	
	<action name="MoveUp">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_E"/>
    </action>
	
	<action name="MoveDown">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_Q"/>
    </action>
 
    <action name="LeftMouseButtonPressed">
   	 <input deviceType="IDV_MOUSE" EventType="EVENT_DOWN"  Code="MOUSE_BUTTON_LEFT"/>
    </action>
	
	<action name="RightMouseButtonPressed">
   	 <input deviceType="IDV_MOUSE" EventType="EVENT_DOWN"  Code="MOUSE_BUTTON_RIGHT"/>
    </action>
	
	<action name="MoveYaw">
	 <input deviceType="IDV_MOUSE" AxisType="AXIS_MOUSE_X" EventType="EVENT_DOWN" Code="MOUSE_BUTTON_LEFT"/>
    </action>
	
	<action name="MovePitch">
   	 <input deviceType="IDV_MOUSE" AxisType="AXIS_MOUSE_Y" EventType="EVENT_DOWN" Code="MOUSE_BUTTON_LEFT"/>
    </action>
	
	<action name="Run">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_LSHIFT"/>
    </action>

 
    <action name="Logger">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_LCTRL"/>
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN_UP"  Code="KEY_F1"/>
    </action>

	<action name="Forward">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_UP"/>
    </action>
	
	<action name="Backward">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_DOWN"/>
    </action>
	
	<action name="Left">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_LEFT"/>
    </action>
	
	<action name="Right">
   	 <input deviceType="IDV_KEYBOARD" EventType="EVENT_DOWN"  Code="KEY_RIGHT"/>
    </action>
</Actions>